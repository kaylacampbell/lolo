<script>
var button = document.getElementById("button");
var d3 = document.getElementById("d3");
function openNav() {
  button.style.display="none";
  d3.style.display="block";
}

function closeNav() {
  button.style.display="inline-block";
  d3.style.display="none";
}
</script>
